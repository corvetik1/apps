{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "outDir": "./dist",
    "types": ["node"],
    "typeRoots": ["../../node_modules/@types"]
  },
  "include": ["schema-generator.ts"],
  "exclude": ["node_modules"]
}
